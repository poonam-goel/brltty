printf "%s\n" "$as_me: pcm includes: ${pcm_includes}" >&6;}

      pcm_libs=`${PKG_CONFIG} ${pkgconfig_flags_libs} "${pcm_package}"`
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: pcm libs: ${pcm_libs}" >&5
printf "%s\n" "$as_me: pcm libs: ${pcm_libs}" >&6;}


      break
   }
done

