do
   ${PKG_CONFIG} --exists "${package_specification}" && {
      printf "%s\n" "#define HAVE_PKG_PORTS 1" >>confdefs.h


      ports_package="${package_specification%% *}"
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: ports package: ${ports_package}" >&5
