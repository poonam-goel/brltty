/* end confdefs.h.  */

#include <windows.h>
#include <stdio.h>
#include <errno.h>

int
main (void) {
  HMODULE module;
  HINSTANCE instance;
  if (!(instance = LoadLibrary("ws2_32.dll"))) return 1;
  if (!(module = GetModuleHandle("ws2_32.dll"))) return 2;
  if (!(GetProcAddress(module, "getaddrinfo"))) return 3;
  return 0;
}

_ACEOF
