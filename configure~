      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: polkit libs: ${polkit_libs}" >&5
printf "%s\n" "$as_me: polkit libs: ${polkit_libs}" >&6;}


printf "%s\n" "#define HAVE_POLKIT 1" >>confdefs.h


