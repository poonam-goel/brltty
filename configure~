then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_LIB${brltty_package}" | $as_tr_cpp` 1
_ACEOF

  LIBS="-l${brltty_package} $LIBS"

else $as_nop
  continue
