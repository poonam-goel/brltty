   ac_fn_c_check_header_compile "$LINENO" "eci.h" "ac_cv_header_eci_h" "$ac_includes_default"
if test "x$ac_cv_header_eci_h" = xyes
then :
        brltty_item_entries_speech="${brltty_item_entries_speech} vv-ViaVoice"
