then :
  cat >>confdefs.h <<_ACEOF
#define `printf "%s\n" "HAVE_LIB${curses_package}" | $as_tr_cpp` 1
_ACEOF

  LIBS="-l${curses_package} $LIBS"

else $as_nop
  curses_package=""
fi
ac_cv_lib_${curses_package}=ac_cv_lib_${curses_package}_main

