    conftest$ac_exeext conftest.$ac_ext

fi
{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $ax_cv_have_func_attribute_format" >&5
printf "%s\n" "$ax_cv_have_func_attribute_format" >&6; }

    if test yes = $ax_cv_have_func_attribute_format
then :

